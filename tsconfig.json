{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "preserve",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": ["ESNext", "DOM"],
        "skipLibCheck": true,
        "noEmit": true,
        "baseUrl": "./",
        "paths": {
            "@molix/*": ["packages/*"]
        },
        "types": []
    },
    "include": [
        "examples/**/*.ts",
        "examples/**/*.tsx",
        "examples/**/*.vue",
        "packages/**/*",
        "packages/**/**/**/*.vue",
        "typings/**/*.ts",
        "tests/**/*.ts",
        "tests/**/*.tsx",
        "packages/**/**/**/_test_/*.spec.ts"
, "packages/components/button/style/index.scss"    ]
}
